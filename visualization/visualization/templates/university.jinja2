{% block content%}
<!DOCTYPE html>
<html lang="en">
<head>
    <meta charset="utf-8">
    <meta name="viewport" content="width=device-width, initial-scale=1">
    <title>{project}</title>   
    <style>
    	html, body {

		  	height: 100%
		}
	
		.bar:hover {
  			fill: #b2b2b2
		}

		.gbar:hover{
			fill: #b2b2b2
		}

		.svgContainer {
			margin-left: 80px;
			margin-right: 80px;
			margin-top:50px;
			outline: thin solid #f0f0f0; 
		}

		.text {
			fill: black;
			font: 20px serif;
			font-weight: bold;
			text-anchor: middle;
		}


		.title{
			fill:black;
			font: 20px semi serif;
			text-anchor: middle;
			font-weight: bold;
		}

		div.tooltip {	
		    position: absolute;	
		    z-index: 10;		
		    text-align: center;			
			visibility: hidden;				
		    padding: 8px;				
		    font: 12px sans-serif;		
		    background-color: rgba(0, 0, 0, 0.75);	
		    border: 1px;			
		    border-radius: 6px;
		    color: white;		
}

		circle {
			  fill: rgb(31, 119, 180);
			  fill-opacity: .25;
			  stroke: rgb(31, 119, 180);
			  stroke-width: 1px;
		}

		.leaf circle {
			  fill: #ff7f0e;
			  fill-opacity: 1;
		}
		
		th, td {
    		padding: 10px;
    		border-bottom: 1px solid #ddd;
		}

		tr:nth-child(even) {background-color: #f2f2f2}
		
		table{
			margin-left: 80px;
			margin-right: auto;
			margin-top:50px;
		}

		.axis--x path {
		  display: none;
		}

		div.table{
			margin-left: 80px;
			margin-right: 80px;
			margin-top:100px;
		}

		caption {
		    text-align: center;
		    margin-bottom: 10px;
		    padding: 8px;
		    font: 20px semi serif;
		    font-weight: bold;

}

    </style> 
</head>
<body> <!-- content -->
    
    <script src="https://d3js.org/d3.v4.min.js"></script>
    <script src="https://d3js.org/d3-selection-multi.v1.min.js"></script>
    <script type ="text/javascript" src="/static/d3.layout.cloud.js"></script>
    <script src="https://ajax.googleapis.com/ajax/libs/jquery/2.1.3/jquery.min.js"></script>
    <script type="text/javascript" src="/static/viz.js"></script>

</body>
</html>
{% endblock content %}
