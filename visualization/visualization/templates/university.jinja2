{% block content%}
<!DOCTYPE html>
<html lang="en">
<head>
    <meta charset="utf-8">
    <meta name="viewport" content="width=device-width, initial-scale=1">
    <title>{project}</title>   
    <style>
    	html, body {
		 	margin: 0;
		  	height: 100%
		}
		.axis {
   			font: 12px sans-serif;
 		}

		.axis path,
 		.axis line {
   			fill: none;
   			stroke: #000;
   			shape-rendering: crispEdges;
 		}
		.bar {
			fill: #0080FF
		}
		.bar:hover {
  			fill: #003366
		}
		.svgContainer {
			padding-left: 100px
		}
		.text {
			fill: "black"
			font-family: sans-serif
		}
    </style> 
</head>
<body> <!-- content -->
    <h1 id="title">In-Part University Reports</h1>        
    <script src="https://d3js.org/d3.v4.min.js"></script>
    <script src="https://d3js.org/d3-selection-multi.v1.min.js"></script>
    <script type="text/javascript" src="/static/viz.js"></script>

</body>
</html>
{% endblock content %}
