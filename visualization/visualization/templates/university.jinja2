{% block content%}
<!DOCTYPE html>
<html lang="en">
<head>
    <meta charset="utf-8">
    <meta name="viewport" content="width=device-width, initial-scale=1">
    <title>{project}</title>   
    <style>
    	html, body {

		  	height: 100%
		}
		
		.title {
			margin-top: 50px;
			margin-left:20px;
		}


		.bar:hover {
  			fill: brown
		}

		.gbar:hover{
			fill: #d0743c
		}

		.svgContainer {
			margin-left: 80px;
			margin-right: 80px;
			margin-top:50px;

		}

		.text {
			fill: black;
			font: 10px sans-serif;
			font-weight: bold;
			text-anchor: middle;
		}

		.title{
			fill:black;
			font: sans-serif
			font-size: 20px;
			font-weight: bold;
			text-anchor: middle;

		}

		div.tooltip {	
		    position: absolute;			
		    text-align: center;			
		    width: 60px;					
		    height: 28px;					
		    padding: 2px;				
		    font: 12px sans-serif;		
		    background: lightsteelblue;	
		    border: 0px;		
		    border-radius: 8px;			
		    pointer-events: none;			
}



		circle {
			  fill: rgb(31, 119, 180);
			  fill-opacity: .25;
			  stroke: rgb(31, 119, 180);
			  stroke-width: 1px;
		}

		.leaf circle {
			  fill: #ff7f0e;
			  fill-opacity: 1;
		}
		
		th, td {
    		padding: 10px;
    		border-bottom: 1px solid #ddd;
		}

		tr:nth-child(even) {background-color: #f2f2f2}
		
		table{
			margin-left: 80px;
			margin-right: auto;
			margin-top:50px;
		}


    </style> 
</head>
<body> <!-- content -->
    <h1 class="title">In-Part University Reports</h1>            
    <script src="https://d3js.org/d3.v4.min.js"></script>
    <script src="https://d3js.org/d3-selection-multi.v1.min.js"></script>
    <script type ="text/javascript" src="/static/d3.layout.cloud.js"></script>
    <script src="https://ajax.googleapis.com/ajax/libs/jquery/2.1.3/jquery.min.js"></script>
    <script type="text/javascript" src="/static/viz.js"></script>

</body>
</html>
{% endblock content %}
